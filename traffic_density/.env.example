# AWS Configuration for SNS Notifications
# Copy this file to .env and fill in your values

# AWS Credentials (or use AWS CLI configuration)
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here
AWS_DEFAULT_REGION=us-east-1

# SNS Topic ARN (required for notifications)
AWS_SNS_TOPIC_ARN=arn:aws:sns:us-east-1:123456789012:traffic-alerts

# Example Topic ARN format:
# arn:aws:sns:<region>:<account-id>:<topic-name>
